        "type": "Group",
        "config": {
          "fields": {
            "single_snippet": {
              "type": "StructuredText",
              "config": {
                "multi": "preformatted",
                "label": "Single snippet"
              }
            }
          },
          "label": "schema.org snippets"
        }
      },
